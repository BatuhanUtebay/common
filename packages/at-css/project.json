{
    "name": "at-css",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/at-css/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@iyio/nx-common:lib-builder",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/at-css",
                "main": "packages/at-css/src/index.ts",
                "tsConfig": "packages/at-css/tsconfig.lib.json",
                "assets": ["packages/at-css/*.md"]
            }
        },
        "publish": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node tools/scripts/publish.mjs at-css {args.ver} {args.tag}"
            },
            "dependsOn": ["build"]
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/at-css/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/at-css/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        }
    },
    "tags": []
}
