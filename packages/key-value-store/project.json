{
    "name": "key-value-store",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/key-value-store/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/key-value-store",
                "main": "packages/key-value-store/src/index.ts",
                "tsConfig": "packages/key-value-store/tsconfig.lib.json",
                "assets": ["packages/key-value-store/*.md"]
            }
        },
        "publish": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node tools/scripts/publish.mjs key-value-store {args.ver} {args.tag}"
            },
            "dependsOn": [
                {
                    "projects": "self",
                    "target": "build"
                }
            ]
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/key-value-store/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/packages/key-value-store"],
            "options": {
                "jestConfig": "packages/key-value-store/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
